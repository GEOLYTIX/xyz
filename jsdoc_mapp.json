{
  "source": {
    "include": [
      "lib"
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  
  "opts": {
    "encoding": "utf8",
    "readme": "./lib/README.md",
    "destination": "docs/mapp/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "Mapp",
      "homepageTitle": "Mapp",
      "favicon": "../public/icons/favicon.ico",
      "menu": [
        {
            "title": "Github",
            "id": "github",
            "link": "https://github.com/GEOLYTIX/xyz"
        },
        {
          "title": "XYZ",
          "id": "xyz",
          "link": "/docs"
        }
      ]
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}