    "HERE Imagery": {
      "group": "Base Maps",
      "format": "tiles",
      "URI": "https://2.aerial.maps.ls.hereapi.com/maptile/2.1/maptile/newest/satellite.day/{z}/{x}/{y}/256/png8?{HERE}",
