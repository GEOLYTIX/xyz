{
  "source": {
    "include": [
      "api", "mod"
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "readme": "./api/README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "XYZ",
      "homepageTitle": "XYZ",
      "static_dir": [
        "./public/icons"
      ],
      "favicon": "./public/icons/favicon.ico",
      "menu": [
        {
          "title": "Github",
          "id": "github",
          "link": "https://github.com/GEOLYTIX/xyz"
        },
        {
          "title": "Mapp",
          "id": "mapp",
          "link": "/docs/mapp"
        }
      ]
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}