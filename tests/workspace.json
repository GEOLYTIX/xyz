    "plugins": [
      "/test/api/provider/file?content_type=application/javascript&url=./tests/assets/plugins/assignMapview.js",
      "/test/api/provider/file?content_type=application/javascript&url=./tests/assets/plugins/test.js"
    ],
    "syncPlugins": [
      "zoomBtn",
      "zoomToArea",
      "coordinates",
      "admin",
